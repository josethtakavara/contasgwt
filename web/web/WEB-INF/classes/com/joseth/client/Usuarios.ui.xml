<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">

  <ui:style>
    .cellTable {
      border-bottom: 11px solid #ccc;
      text-align: left;
      margin-bottom: 4px;
    }
  </ui:style>

  <g:HTMLPanel>
    <g:HorizontalPanel ui:field="pnUsuarios" width="10%" styleName="{style.cellTable}">
    </g:HorizontalPanel>
    <g:HorizontalPanel>
	    <g:PushButton ui:field="classAddBt"><g:upFace><img src="img/plus.png"/></g:upFace></g:PushButton>
		<g:PushButton ui:field="classRemBt"><g:upFace><img src="img/excluir.gif"/></g:upFace></g:PushButton>
	</g:HorizontalPanel>
  </g:HTMLPanel>
</ui:UiBinder>