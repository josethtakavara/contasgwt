<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">

  <ui:style>
    .cellTable {
      border-bottom: 11px solid #ccc;
      text-align: left;
      margin-bottom: 4px;
    }
  </ui:style>
  
  <g:HTMLPanel>
    <g:HorizontalPanel width="10%" styleName="{style.cellTable}">
        <g:ListBox ui:field="conta"/>
    </g:HorizontalPanel>
    <g:HorizontalPanel>
         <g:RadioButton ui:field="tpArquivo"/>
         <g:RadioButton ui:field="tpDados"/>
	  </g:HorizontalPanel>
	  <g:HorizontalPanel ui:field="entrada">
    </g:HorizontalPanel>
    <g:HorizontalPanel ui:field="visualizar" height="100">
    </g:HorizontalPanel>
  </g:HTMLPanel>
</ui:UiBinder>